#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/is-ci@3.0.1/node_modules/is-ci/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/is-ci@3.0.1/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules:/Users/steventravers/Documents/Perso/node_modules:/Users/steventravers/Documents/node_modules:/Users/steventravers/node_modules:/Users/node_modules:/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/is-ci/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/is-ci@3.0.1/node_modules/is-ci/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/is-ci@3.0.1/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules:/Users/steventravers/Documents/Perso/node_modules:/Users/steventravers/Documents/node_modules:/Users/steventravers/node_modules:/Users/node_modules:/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/is-ci/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-ci/bin.js" "$@"
else
  exec node  "$basedir/../is-ci/bin.js" "$@"
fi
