#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/@babel+parser@7.16.2/node_modules/@babel/parser/bin/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/@babel+parser@7.16.2/node_modules/@babel/parser/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/@babel+parser@7.16.2/node_modules/@babel/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/@babel+parser@7.16.2/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules:/Users/steventravers/Documents/Perso/node_modules:/Users/steventravers/Documents/node_modules:/Users/steventravers/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/@babel+parser@7.16.2/node_modules/@babel/parser/bin/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/@babel+parser@7.16.2/node_modules/@babel/parser/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/@babel+parser@7.16.2/node_modules/@babel/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/@babel+parser@7.16.2/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules:/Users/steventravers/Documents/Perso/node_modules:/Users/steventravers/Documents/node_modules:/Users/steventravers/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../bin/babel-parser.js" "$@"
fi
