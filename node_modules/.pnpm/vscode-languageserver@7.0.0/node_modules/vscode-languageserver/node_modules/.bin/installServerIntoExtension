else
  export NODE_PATH="$NODE_PATH:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules/vscode-languageserver/bin/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules/vscode-languageserver/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules:/Users/steventravers/Documents/Perso/node_modules:/Users/steventravers/Documents/node_modules:/Users/steventravers/node_modules:/Users/node_modules:/node_modules"
fi
"$basedir/../../bin/installServerIntoExtension"   "$@"
exit $?
