#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/husky@7.0.4/node_modules/husky/lib/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/husky@7.0.4/node_modules/husky/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/husky@7.0.4/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules:/Users/steventravers/Documents/Perso/node_modules:/Users/steventravers/Documents/node_modules:/Users/steventravers/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/husky@7.0.4/node_modules/husky/lib/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/husky@7.0.4/node_modules/husky/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/husky@7.0.4/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules/.pnpm/node_modules:/Users/steventravers/Documents/Perso/FoodAndThought/node_modules:/Users/steventravers/Documents/Perso/node_modules:/Users/steventravers/Documents/node_modules:/Users/steventravers/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
